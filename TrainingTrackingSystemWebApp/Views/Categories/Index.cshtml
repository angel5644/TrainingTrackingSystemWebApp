@model IEnumerable<TrainingTrackingSystemWebApp.ViewModels.Categories.CategoryViewModel>
@{
    ViewBag.Title = "Index";

}
<h2>Index</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
        </tr>

    </thead>
    <tbody>
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.Name</td>
                <td>@category.Description</td>
            </tr>
        }
    </tbody>
</table>

